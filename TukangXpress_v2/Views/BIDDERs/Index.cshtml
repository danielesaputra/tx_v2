@model IEnumerable<TukangXpress_v2.Models.BIDDER>

@{
    ViewBag.Title = "Index";
}

<h2>Bidder</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Bidder ID
        </th>
        <th>
            Vendor ID
        </th>
        <th>
            Project ID
        </th>
        <th>
            Ammount
        </th>
        <th>
            Winning Status
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.bidder_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VENDOR_DATA.vendordata_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROJECT_MASTER.project_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ammount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.winning_status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.bidder_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.bidder_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.bidder_ID })
        </td>
    </tr>
}

</table>
