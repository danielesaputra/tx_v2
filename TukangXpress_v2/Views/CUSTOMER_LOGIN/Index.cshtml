@model IEnumerable<TukangXpress_v2.Models.CUSTOMER_LOGIN>

@{
    ViewBag.Title = "Index";
}

<h2>Customer Login</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Login ID
        </th>
        <th>
            Email
        </th>
        <th>
            Password
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.customerlogin_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.customer_email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.customer_password)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.customerlogin_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.customerlogin_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.customerlogin_ID })
        </td>
    </tr>
}

</table>
